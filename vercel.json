{
  "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://chatappbackend-production-3fab.up.railway.app/api/"
    },
    {
      "source": "/socket.io/(.*)",
      "destination": "https://chatappbackend-production-3fab.up.railway.app/socket.io/"
    }
  ]
}
